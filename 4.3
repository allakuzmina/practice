#include <iostream>
#include <time.h>
 
using namespace std;
 
void Random(int **mass, int a, int b)
{
    
 
    for(int i=0; i<a; i++)
    for(int j=0; j<b; j++)
      {
        mass[i][j] = rand()  % (50-10+1)+10 ;
      }
 
    }
 
void Show(int **mass, int a, int b)
{
    for(int i=0; i<a; i++)
    {
    for(int j=0; j<b; j++)
    {
      cout << "   " << mass[i][j]<< "  ";
    }
        cout << endl;
    }
}
 
void Delete(int **mass, int a)
{
    for (int i=0; i<a; i++)
    {
        delete[] mass[i];
    }
    delete[] mass;
}
 
int main()
{
    int a,b;
 
    cout << "Size: ";
    cin >> a >> b;
 
    int **mass = new int *[a];
 
    for (int i=0; i<a; i++)
    {
        mass[i] = new int[b];
    }
 
    Random(mass, a, b);
    Show(mass, a, b);
    Delete(mass, a);
 
    return 0;
}
